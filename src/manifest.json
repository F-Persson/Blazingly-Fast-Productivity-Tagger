{
    "name": "Blazingly Fast Productivity Tagger",
    "description": "Ultimate productivty booster for your notes and bookmarks",
    "version": "0.5.3",
    "manifest_version": 3,
    "author": "Francis P",
    "background": {
        "service_worker": "assets/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "assets/content.js"
            ]
        }
    ],
    "options_page": "index.html#options"
}